#  neighbor-domains:
#    Kanadzuchi::Mail::Group::Neighbor reads this file(YAML). If you use 
#    this file, it should be saved as 'neighbor-domains'.
#
# SYNOPSIS:
#  provider-name: [ 'domain1', 'domain2', ... ]
#
# EXAMPLE:
#    snakeoil: [ 'snake.example.jp', 'hebi.example.jp', 'nyoro.example.jp' ]
#    oursites: [ 'snakeoil-online.example.com', 'snakeoil-sales.example.com' ]

